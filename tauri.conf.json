{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "leda-tauri",
  "version": "0.1.0",
  "identifier": "leda-app.seadeep.ai",
  "build": {
    "frontendDist": "./dist",
    "devUrl": "http://127.0.0.1:8000",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "leda app",
        "width": 1920,
        "height": 1080,
        "resizable": true,
        "fullscreen": false,
        "dragDropEnabled": true,
        "url": "http://127.0.0.1:8000"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "leda_app"
    ]
  }
}
